contents of catalog.wsgi

#!/usr/bin/python
import sys
import logging
logging.basicConfig(stream=sys.stderr)
sys.path.insert(0,"/var/www/catalog/")

from catalogshoe import app as application
application.secret_key = 'ncAkvL1QSygeFQcjOVFCrC5M'

